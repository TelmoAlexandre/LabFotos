@model LabFoto.Models.Tables.ServicoSolicitado

<div class="ui raised segment fluidCorners">
    <a class="ui blue ribbon label mb-3">Novo Serviço Solicitado</a>
    <div class="ui form">
        @Html.AntiForgeryToken()
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="field">
            <label asp-for="Nome"></label>
            <input asp-for="Nome" placeholder="Serviço Solicitado" />
            <span asp-validation-for="Nome" class="text-danger"></span>
        </div>
        <div class="fields">
            <div class="eight wide field"><button type="submit" class="btn btn-block btn-primary" id="btnAddServSolic" onclick="createServSolic()">Adicionar</button></div>
            <div class=" eight wide field"><span class="btn btn-block btn-secondary pointer" onclick="hideModal()">Cancelar</span></div>
        </div>
    </div>
</div>