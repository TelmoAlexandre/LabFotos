@model LabFoto.Models.ViewModels.ServicosCreateViewModel

<div class="ui multiple search selection dropdown">
    <input type="hidden" name="Tipos">
    <i class="dropdown icon"></i>
    <div class="default text">Selecione o(s) tipo(s)</div>
    <div class="menu">
        @foreach (var tipo in Model.TiposList)
        {
            <div class="item" data-value="@tipo.Value">@tipo.Text</div>
        }
    </div>
</div>